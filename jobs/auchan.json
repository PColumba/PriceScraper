{
  "jobId": "prices_auchan",
  "baseURL": "https://www.auchandirect.pl/",
  "pageParam": "page",
  "pageOffset": 0,

  "priceValueExtractor": {
    "elementSelector": "p.packaging",
    "elementTextPattern": "^(?:\\s*)([0-9]+,?[0-9]*)(?:z≈Ç)"
  },

  "priceTypeExtractor": {
    "elementSelector": "p.packaging",
    "elementTextPattern": "(?:/\\s)([A-Za-z]+\\.?)$"
  },

  "productIdExtractor": {
    "elementSelector": "a.about",
    "attributeName": "onclick",
    "attributeTextPattern": "(?:\\(')([0-9]+)(?:')"
  },

  "productNameExtractor": {
    "elementSelector": "a.about",
    "elementChildText": true
  },

  "productsListExtractor": {
    "elementSelector": "#search-product-list article.product"
  },

  "pageCountExtractor": {
    "elementSelector": "#loadMoreProductsButton",
    "attributeName": "data-maxpage"
  },

